		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"karma": "^2.0.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-cljs-test": "^0.1.0",
		"shadow-cljs": "2.14.5",
		"workbox-cli": "^3.4.1"
	}
